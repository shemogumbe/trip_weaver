files:
  "/etc/nginx/conf.d/proxy.conf":
    mode: "000644"
    owner: root
    group: root
    content: |
      proxy_buffering off;
      gzip off;
      proxy_read_timeout 600s;
      proxy_send_timeout 600s;
      chunked_transfer_encoding on;
      set_real_ip_from 0.0.0.0/0;
      real_ip_header X-Forwarded-For;
